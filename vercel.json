{
    "version": 2,
    "builds": [
        {
            "src": "api/collect.js",
            "use": "@vercel/node"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/collect",
            "dest": "/api/collect.js"
        },
        {
            "src": "/",
            "dest": "/index.html"
        }
    ]
}